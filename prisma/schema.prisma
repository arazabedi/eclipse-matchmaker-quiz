datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  title   String
  date    DateTime @default(now())
  content String
}

model User {
	id      String   @id @default(auto()) @map("_id") @db.ObjectId
  name            String
  email           String
  gender          String
  preferredGender String
	responses Int[]
}
